    "i18next-http-backend": "^2.5.2",
    "js-sha256": "^0.11.0",
    "lodash-es": "^4.17.21",
    "pdfmake": "^0.2.10",
    "react": "^18.2.0",
    "react-cookie": "^7.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
